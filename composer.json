{
    "name": "artisanry/recaptcha",
    "description": "google/recaptcha Wrapper for Laravel 5.",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-messageable",
        "Laravel Messageable",
        "laravel-recaptcha"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8",
        "google/recaptcha": "^1.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Recaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Recaptcha\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Recaptcha\\RecaptchaServiceProvider"
            ],
            "aliases": {
                "Recaptcha": "Artisanry\\Recaptcha\\Facades\\Recaptcha"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
