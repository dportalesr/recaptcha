{
    "name": "faustbrian/laravel-recaptcha",
    "description": "google/recaptcha Wrapper for Laravel 5.",
    "keywords": ["laravel", "framework", "Laravel-messageable", "Laravel Messageable", "Brian Faust", "faustbrian", "laravel-recaptcha"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.*",
        "google/recaptcha": "^1.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Recaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Recaptcha\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Recaptcha\\RecaptchaServiceProvider"
            ],
            "aliases": {
                "Recaptcha": "BrianFaust\\Recaptcha\\Facades\\Recaptcha"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
